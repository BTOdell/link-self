{
  "compileOnSave": true,
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "module": "CommonJS",
    "target": "ES5",
    "lib": [
      "ES5"
    ],
    "types": ["node"],
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "removeComments": true,
    "strict": true,
    "noImplicitReturns": true
  }
}
